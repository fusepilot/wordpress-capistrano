#  !!!
#  DO NOT EDIT THIS FILE DIRECTLY ON THE SERVER
#  !!
#  Edit it in your wordpress-capistrano repo, commit and push it,
#  then run cap nginx:configure to update it on the server.

server {
  listen          80;
  server_name     <%= domains %>;
  error_log	      /var/log/<%= domain %>.error.log debug;
	access_log      /var/log/<%= domain %>.access.log;
	root            <%= deploy_to %>/current;

	include wordpress_params;
}